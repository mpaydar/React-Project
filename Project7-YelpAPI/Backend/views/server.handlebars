<h1>{{title}}</h1>
<form action="/api/members" method="post">
    <div class="form-group">
        <label for="name">Name</label>
        <input type="text" name="name" class="form-controll">
    </div>
    <div class="form-group">
        <label for="emai">Email</label>
        <input type="email" name="email" class="form-controll">
    </div>

    <input type="submit" value="Add Member" class="btn btn-primary btn-block">

</form>
<h3>Members</h3>
<ul>
    {{#each members}}
    <li>{{this.name}}:{{this.email}}</li>
    {{/each}}

</ul>

<a href="/api/members" class="btn btn-light mt-4">Visit API</a>